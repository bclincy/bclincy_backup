<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %}</title>
    {% block css %}
    <link href="{{ asset('css/default.css') }}" rel="stylesheet" />
    {% endblock css %}
  </head>
  <body>
  <header>
      <nav class="navbar navbar-default">
          <div class="container">
              <div class="navbar-header">
                  <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                          aria-expanded="false" aria-controls="navbar">
                      <span class="sr-only">Toggle navigation</span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                  </button>
                  <a class="navbar-brand" href="#">Brian Clincy</a>
              </div>
              <div id="navbar" class="collapse navbar-collapse">
                  <ul class="nav navbar-nav">
                      <li class="active"><a href="#">Home</a></li>
                      <li><a href="#about">About</a></li>
                      <li><a href="#contact">Contact</a></li>
                  </ul>
                  <ul class="nav navbar-nav navbar-right">
                      <li><a href="http://twitter.com/clincy"><i class="fa fa-twitter"></i></a></li>
                      <li><a href="http://facebook.com/clincy"><i class="fa fa-facebook"></i></a></li>
                      <li><a href="https://plus.google.com/+BrianClincy"><i class="fa fa-google-plus"></i></a></li>
                      <li><a href="http://linkedin.com/bclincy"><i class="fa fa-linkedin"></i></a></li>
                  </ul>
              </div>
              <!--/.nav-collapse -->
          </div>
      </nav>
  </header>
  {% block content %}
    <h1>Hello Olivia</h1>
        {{ dump() }}
    {% endblock content %}

    {% block javascripts %}
      <script src="{{ asset('js/main.js') }}"></script>
    {% endblock %}

  </body>
</html>
